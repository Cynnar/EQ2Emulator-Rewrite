<eq2emu>
    <!--
         host: This is the interface the server will listen on: There are 3 different types of values:
               1) 'localhost' or '127.0.0.1' = This is your loopback interface and will only let your machine connect.
               2) 'x.x.x.x' = The actual IP address of the interface you want to bind to.
         port: The port your server will listen on.
    -->
    <!--<server host="127.0.0.1" port="9100" />-->
	
	<zonetalk host="127.0.0.1" port="9105" />
    
    <!-- Credentials to use to connect to the MySQL server -->
    <!--<database host="127.0.0.1" port="3306" user="rewrite" password="1234" database="eq2_rewrite" />-->

    <!--
        Credentials for the address of the patch server to connect to.
        host: The IP address of patch server.
        port: The port of the patch server.
        enabled: If false, the server won't try to connect to the patch server.
    -->
    <!-- <patch host="updates.eq2emulator.net" port="9104" enabled="true" /> -->
	<patch host="updates.eq2emulator.net" port="9104" enabled="false" />
    
